      - eventKey: OnRoomStart
        code: >-
          ct.sound.spawn('war_drums', {
              loop: true,
              volume: 0.50
          });


          inGameRoomStart(this);


          this.whirlpoolDisabled = false;


          this.win = false;


          // this.TotalBoatsInLevel = 10;

          // this.spawnedBoats = 0;


          // drawing and managing score

          this.score = 0; // score for tallying points and trigger when the next
          special attack is available

          this.scoreLabel = new PIXI.Text('Score: ' + this.score);

          // this.addChild(this.scoreLabel);

          // this.scoreLabel.x = 350;

          // this.scoreLabel.y = 10;

          // this.scoreLabel.depth = 1000;


          // drawing and managing lives

          this.lives = 3;

          this.livesLabel = new PIXI.Text('Flowers Remaining: ' + this.lives,
          ct.styles.get('lives'));

          this.addChild(this.livesLabel);

          this.livesLabel.x = 100;

          this.livesLabel.y = 670;

          this.livesLabel.depth = 1000;


          // draws day counter ('level counter')

          // this.day = 1;

          // this.dayLabel = new PIXI.Text('Day: ' + this.day);

          // this.addChild(this.dayLabel);

          // this.dayLabel.x = 800;

          // this.dayLabel.y = 10;

          // this.dayLabel.depth = 1000;


          // generate boats over time

          this.timer1 = 1; // first wave of boats

          this.timer2 = 30; // second wave of boats (faster)

          this.timer3 = 60; // third wave of boats (fastest)


          ct.transition.fadeIn();
        arguments: {}
        lib: core
      - eventKey: OnDraw
        code: |-
          // updates score and lives
          this.scoreLabel.text = 'Score: ' + this.score;
          this.livesLabel.text = 'Flowers Remaining: ' + this.lives;

          // initiate 'you lost' screen if player loses all lives
          if (this.lives <= 0) {
              // this.timer1.reject();
              ct.u.wait(200)
              .then(() => {
                  // Add a layer with "Lose" UI
                  // ct.rooms.append('UI_Lost', {
                  //     isUi: true
                  // });

                  //ct.pixiApp.ticker.speed = 0;
                  ct.sound.stop('war_drums');
                  ct.rooms.switch('UI_Lost');
              }).catch(() => void 0);
          }

          if (this.win == true) {
                  ct.u.wait(200)
              .then(() => {
                  //ct.pixiApp.ticker.speed = 0;
                  ct.sound.stop('war_drums');
                  ct.rooms.switch('win');
              }).catch(() => void 0);
          }
        arguments: {}
        lib: core
      - eventKey: Timer1
        code: >-
          // while (this.spawnedBoats < this.TotalBoatsInLevel) {

          //     ct.templates.copy('boat', ct.camera.right + 50,
          ct.random.range(ct.camera.bottom - 50, ct.camera.bottom - 140));

          //     this.spawnedBoats ++;

          //     // console.log(this.spawnedBoats);

          //     // if (this.spawnedBoats == this.TotalBoatsInLevel) {

          //     //     return;

          //     // }

          //     this.timer1 = ct.random.range(0.4, 1);

          // }

          if (!this.secondWaveStart) {
              this.timer1 = ct.random.range(0.5, 2);
              ct.templates.copy('boat-01', ct.camera.right + 50, ct.random.range(ct.camera.bottom - 50, ct.camera.bottom - 300));
          }
        arguments:
          name: boats
          isUi: false
        lib: core
      - eventKey: OnActionPress
        code: |-
          // creates whirlpool when in-game screen is not paused
          if (ct.pixiApp.ticker.speed != 0 && !this.whirlpoolDisabled) {
              ct.templates.copy('whirlpool', 800, 500);
          }
        arguments:
          action: createWhirlpool
        lib: core
      - eventKey: OnActionPress
        code: |-
          // creates lightning when in game screen is not paused
          if (ct.pixiApp.ticker.speed != 0) {
              ct.templates.copy('bolt1', ct.pointer.x, ct.pointer.y);
          }
        arguments:
          action: createLightning
        lib: core
      - eventKey: Timer2
        code: |-
          this.secondWaveStart = true;
          if (!this.thirdWaveStart) {
              this.timer2 = ct.random.range(2, 5);
              ct.templates.copy('boat-02', ct.camera.right + 50, ct.random.range(ct.camera.bottom - 60, ct.camera.bottom - 300));
          }
        arguments:
          name: 2ndwave
          isUi: false
        lib: core
      - eventKey: Timer3
        code: >-
          this.thirdWaveStart = true;


          //this.timer3 = ct.random.range(0.2, 0.5);

          ct.templates.copy('boat-03', ct.camera.right + 50,
          ct.random.range(ct.camera.bottom - 50, ct.camera.bottom - 300));
        arguments:
          name: 3rdwave
          isUi: false
        lib: core
      - eventKey: OnRoomEnd
        code: ct.sound.stop('war_drums');
        arguments: {}
        lib: core
