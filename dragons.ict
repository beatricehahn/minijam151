  - name: LevelManager
    code: |-
      var LevelManager = function (currentLevel) {
          const BaselineEnemiesPerLevel = 10;

          if (currentLevel == 1) {
              return BaselineEnemiesPerLevel;
          } else if (currentLevel == 2) {
              return BaselineEnemiesPerLevel * 2;
          } else if (currentLevel == 3) {
              return BaselineEnemiesPerLevel * 3;
          }
      }
